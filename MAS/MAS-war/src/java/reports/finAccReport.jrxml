     booking.`UPDATEDTIME` AS booking_UPDATEDTIME,
     booking.`AREACODE` AS booking_AREACODE,
     booking.`COUNTRYCODE` AS booking_COUNTRYCODE,
     booking.`NUMBER` AS booking_NUMBER,
     booking.`BOOKINGTRANS_ID` AS booking_BOOKINGTRANS_ID,
     booking.`claimed` AS booking_claimed,
     booking.`promoPrice` AS booking_promoPrice,
     flight.`ID` AS flight_ID,
     flight.`COMPLETED` AS flight_COMPLETED,
     flight.`DELETED` AS flight_DELETED,
     flight.`FLIGHTNO` AS flight_FLIGHTNO,
     flight.`NUMOFUNSCHEDULED` AS flight_NUMOFUNSCHEDULED,
     flight.`SPEEDFRACTION` AS flight_SPEEDFRACTION,
     flight.`WEEKLYFREQUENCY` AS flight_WEEKLYFREQUENCY,
     flight.`ROUTE_ROUTEID` AS flight_ROUTE_ROUTEID,
     flight.`RETURNEDFLIGHT_ID` AS flight_RETURNEDFLIGHT_ID,
     flight.`flyDistance` AS flight_flyDistance,
     flightcrew.`SYSTEMUSERID` AS flightcrew_SYSTEMUSERID,
     flightcrew.`BASE` AS flightcrew_BASE,
     flightcrew.`DATEJOINED` AS flightcrew_DATEJOINED,
     flightcrew.`DOB` AS flightcrew_DOB,
     flightcrew.`FLIGHTCREWID` AS flightcrew_FLIGHTCREWID,
     flightcrew.`GENDER` AS flightcrew_GENDER,
     flightcrew.`TOTALFLYINGDIST` AS flightcrew_TOTALFLYINGDIST,
     flightcrew.`TOTALFLYINGTIME` AS flightcrew_TOTALFLYINGTIME,
     flightcrew.`POSITION_ID` AS flightcrew_POSITION_ID,
     flightschedule.`FLIGHTSCHEDULEID` AS flightschedule_FLIGHTSCHEDULEID,
     flightschedule.`ACTUALARRIVALDATE` AS flightschedule_ACTUALARRIVALDATE,
     flightschedule.`ACTUALDEPARTDATE` AS flightschedule_ACTUALDEPARTDATE,
     flightschedule.`ARRIVALDATE` AS flightschedule_ARRIVALDATE,
     flightschedule.`ARRIVALGATE` AS flightschedule_ARRIVALGATE,
     flightschedule.`ARRIVALTERMINAL` AS flightschedule_ARRIVALTERMINAL,
     flightschedule.`COMPLETED` AS flightschedule_COMPLETED,
     flightschedule.`CREATEDTIME` AS flightschedule_CREATEDTIME,
     flightschedule.`DELETED` AS flightschedule_DELETED,
     flightschedule.`DEPARTDATE` AS flightschedule_DEPARTDATE,
     flightschedule.`DEPARTGATE` AS flightschedule_DEPARTGATE,
     flightschedule.`DEPARTTERMINAL` AS flightschedule_DEPARTTERMINAL,
     flightschedule.`PRICED` AS flightschedule_PRICED,
     flightschedule.`SEATALLOCATED` AS flightschedule_SEATALLOCATED,
     flightschedule.`STATUS` AS flightschedule_STATUS,
     flightschedule.`TURNOVERTIME` AS flightschedule_TURNOVERTIME,
     flightschedule.`AIRCRAFT_AIRCRAFTID` AS flightschedule_AIRCRAFT_AIRCRAFTID,
     flightschedule.`FLIGHT_ID` AS flightschedule_FLIGHT_ID,
     flightschedule.`LEG_LEGID` AS flightschedule_LEG_LEGID,
     flightschedule.`NEXTFLIGHTSCHED_FLIGHTSCHEDULEID` AS flightschedule_NEXTFLIGHTSCHED_FLIGHTSCHEDULEID,
     flightschedule.`PREFLIGHTSCHED_FLIGHTSCHEDULEID` AS flightschedule_PREFLIGHTSCHED_FLIGHTSCHEDULEID,
     mktcampaign.`ID` AS mktcampaign_ID,
     mktcampaign.`AUDIENCESIZE` AS mktcampaign_AUDIENCESIZE,
     mktcampaign.`BUDGET` AS mktcampaign_BUDGET,
     mktcampaign.`CAMPAIGNRESULT` AS mktcampaign_CAMPAIGNRESULT,
     mktcampaign.`DESCRIPTION` AS mktcampaign_DESCRIPTION,
     mktcampaign.`ENDTIME` AS mktcampaign_ENDTIME,
     mktcampaign.`NAME` AS mktcampaign_NAME,
     mktcampaign.`RESPONDERNO` AS mktcampaign_RESPONDERNO,
     mktcampaign.`REVENUE` AS mktcampaign_REVENUE,
     mktcampaign.`STARTTIME` AS mktcampaign_STARTTIME,
     mktcampaign.`TYPE` AS mktcampaign_TYPE,
     systemuser.`SYSTEMUSERID` AS systemuser_SYSTEMUSERID,
     systemuser.`DTYPE` AS systemuser_DTYPE,
     systemuser.`ACTIVATED` AS systemuser_ACTIVATED,
     systemuser.`ADDRESS` AS systemuser_ADDRESS,
     systemuser.`DELETED` AS systemuser_DELETED,
     systemuser.`DEPARTMENT` AS systemuser_DEPARTMENT,
     systemuser.`EMAIL` AS systemuser_EMAIL,
     systemuser.`LOCKED` AS systemuser_LOCKED,
     systemuser.`LOGGEDIN` AS systemuser_LOGGEDIN,
     systemuser.`NAME` AS systemuser_NAME,
     systemuser.`PASSWORD` AS systemuser_PASSWORD,
     systemuser.`PHONE` AS systemuser_PHONE,
     systemuser.`RESETDIGEST` AS systemuser_RESETDIGEST,
     systemuser.`SALARY` AS systemuser_SALARY,
     systemuser.`STATUS` AS systemuser_STATUS,
     systemuser.`USERNAME` AS systemuser_USERNAME
FROM
     `aircraft` aircraft INNER JOIN `flightschedule` flightschedule ON aircraft.`AIRCRAFTID` = flightschedule.`AIRCRAFT_AIRCRAFTID`
     INNER JOIN `flight` flight ON flightschedule.`FLIGHT_ID` = flight.`ID`,
     `systemuser` systemuser INNER JOIN `flightcrew` flightcrew ON systemuser.`SYSTEMUSERID` = flightcrew.`SYSTEMUSERID`,
     `booking` booking,
     `mktcampaign` mktcampaign]]>
