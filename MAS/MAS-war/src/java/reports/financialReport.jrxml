	<subDataset name="PieChartDataset" uuid="07bc7033-c94f-4cd2-84d4-4e6d5ed76db1"/>
	<queryString language="SQL">
		<![CDATA[SELECT
     AIRCRAFT.`AIRCRAFTID` AS AIRCRAFT_AIRCRAFTID,
     AIRCRAFT.`COST` AS AIRCRAFT_COST,
     AIRCRAFT.`SOURCE` AS AIRCRAFT_SOURCE,
     AIRCRAFT.`STATUS` AS AIRCRAFT_STATUS,
     AIRCRAFT.`TAILNO` AS AIRCRAFT_TAILNO,
     FLIGHT.`ID` AS FLIGHT_ID,
     FLIGHT.`FLYDISTANCE` AS FLIGHT_FLYDISTANCE,
     FLIGHTSCHEDULE.`FLIGHTSCHEDULEID` AS FLIGHTSCHEDULE_FLIGHTSCHEDULEID,
     FLIGHTSCHEDULE.`DELETED` AS FLIGHTSCHEDULE_DELETED,
     FLIGHTSCHEDULE.`FLIGHT_ID` AS FLIGHTSCHEDULE_FLIGHT_ID,
     MKTCAMPAIGN.`BUDGET` AS MKTCAMPAIGN_BUDGET,
     SYSTEMUSER.`SYSTEMUSERID` AS SYSTEMUSER_SYSTEMUSERID,
     SYSTEMUSER.`DTYPE` AS SYSTEMUSER_DTYPE,
     SYSTEMUSER.`DELETED` AS SYSTEMUSER_DELETED,
     SYSTEMUSER.`NAME` AS SYSTEMUSER_NAME,
     SYSTEMUSER.`SALARY` AS SYSTEMUSER_SALARY,
     FLIGHTCREW.`SYSTEMUSERID` AS FLIGHTCREW_SYSTEMUSERID,
     FLIGHTCREW.`FLIGHTCREWID` AS FLIGHTCREW_FLIGHTCREWID,
     FLIGHTCREW.`TOTALFLYINGTIME` AS FLIGHTCREW_TOTALFLYINGTIME,
     MKTCAMPAIGN.`NAME` AS MKTCAMPAIGN_NAME,
     SYSTEMUSER.`DEPARTMENT` AS SYSTEMUSER_DEPARTMENT,
     FLIGHTSCHEDULE.`COMPLETED` AS FLIGHTSCHEDULE_COMPLETED
FROM
     `AIRCRAFT` AIRCRAFT INNER JOIN `FLIGHTSCHEDULE` FLIGHTSCHEDULE ON AIRCRAFT.`AIRCRAFTID` = FLIGHTSCHEDULE.`AIRCRAFT_AIRCRAFTID`
     INNER JOIN `FLIGHT` FLIGHT ON FLIGHTSCHEDULE.`FLIGHT_ID` = FLIGHT.`ID`,
     `SYSTEMUSER` SYSTEMUSER INNER JOIN `FLIGHTCREW` FLIGHTCREW ON SYSTEMUSER.`SYSTEMUSERID` = FLIGHTCREW.`SYSTEMUSERID`,
     `MKTCAMPAIGN` MKTCAMPAIGN]]>
	</queryString>
